{
  "name": "@hodfords/nestjs-cls-translation",
  "version": "10.2.0-rc.1",
  "description": "Provides context-aware translations in NestJS using CLS",
  "main": "index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "postbuild": "cp package.json dist/package.json",
