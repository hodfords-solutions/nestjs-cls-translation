    "cspell": "cspell",
    "prepare": "is-ci || husky",
    "lint": "eslint \"libs/**/*.ts\" --fix --max-warnings 0",
    "lint-staged": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hodfords-solutions/nestjs-cls-translation"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hodfords-solutions/nestjs-cls-translation/issues"
  },
  "homepage": "https://github.com/hodfords-solutions/nestjs-cls-translation#readme",
  "dependencies": {
    "nestjs-cls": "^4.4.1",
    "nestjs-i18n": "10.4.5"
  },
  "devDependencies": {
    "@nestjs/bull": "10.2.1",
    "@nestjs/common": "10.4.1",
    "@nestjs/core": "10.4.1",
    "@types/express": "4.17.21",
    "@types/node": "22.5.0",
    "@typescript-eslint/eslint-plugin": "8.3.0",
    "@typescript-eslint/parser": "8.3.0",
    "cspell": "8.14.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "express": "4.19.2",
    "husky": "9.1.5",
    "is-ci": "3.0.1",
    "lint-staged": "15.2.9",
    "prettier": "3.3.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "6.0.1",
    "rxjs": "7.8.1",
    "typescript": "5.5.4"
  }
